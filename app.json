{
  "expo": {
    "name": "BDPayX",
    "slug": "bdpayx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A90E2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bdpayx.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#4A90E2"
      },
      "package": "com.bdpayx.app"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "scheme": "bdpayx",
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-build-properties",
      "expo-web-browser"
    ]
  }
}
